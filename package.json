{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning Node, Express and MongoDB.",
  "main": "app.js",
  "scripts": {
    "start": "set NODE_ENV=production&&node server.js",
    "start:dev": "nodemon server.js",
    "start:prod": "set NODE_ENV=production&&nodemon server.js",
    "insert": "set NODE_ENV=LOADER&&nodemon insert-dev-data.js --insert",
    "delete": "set NODE_ENV=LOADER&&nodemon insert-dev-data.js --delete",
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
    "build": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "compile:sass": "sass public/css/sass/main.scss public/css/sass_style.css --watch",
    "compile:sass-common": "sass public/css/common_styles/main.scss public/css/common_style.css --watch"
  },
  "author": "Shivam Dhaka",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "aws-sdk": "^2.1475.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.11.0",
    "helmet": "3.6",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "mongoose": "^5.13.20",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "ndb": "^1.1.5",
    "nodemailer": "^6.9.5",
    "parcel-bundler": "^1.12.5",
    "pug": "^3.0.2",
    "s": "^1.0.0",
    "sharp": "^0.32.5",
    "slugify": "^1.6.6",
    "stripe": "^13.6.0",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "prettier": "3.0.3",
    "sass": "^1.69.1"
  },
  "engines": {
    "node": "^18"
  }
}
